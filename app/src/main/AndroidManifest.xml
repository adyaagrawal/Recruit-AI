<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.recruitai">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RecruitAI">
        <activity android:name=".Interview" />
        <activity android:name=".QuestionC" />
        <activity android:name=".FinalAnalysis" />
        <activity android:name=".UserAnalysis" />
        <activity android:name=".JobDetailsCompany" />
        <activity android:name=".MainActivity2" />
        <activity android:name=".UserApply" />
        <activity android:name=".PostJob" />
        <activity android:name=".Company" />
        <activity android:name=".User" />
        <activity android:name=".CompanySignup" />
        <activity android:name=".UserSignup" />
        <activity android:name=".CompanyLogin" />
        <activity android:name=".UserLogin" />
        <activity android:name=".Signup" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

    </application>

</manifest>